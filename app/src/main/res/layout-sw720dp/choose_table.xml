<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:background="@color/white"
    android:layout_height="wrap_content">
    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/layoutdate"
                android:layout_marginLeft="@dimen/dimen_20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvtarih"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tarih"
                    android:textColor="@color/editArka"
                    android:textSize="30dp" />


            </LinearLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/entrancelayout"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="@dimen/dimen_30dp"
                    android:layout_marginRight="@dimen/dimen_10dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_20dp"
                    android:orientation="vertical"
                    >
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <Button
                                android:id="@+id/btnchoose10"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@mipmap/b10"
                                android:padding="@dimen/dimen_45dp"
                                android:onClick="onButtonClicked"
                                android:text=""
                                android:textColor="@android:color/black" />


                        </LinearLayout>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dimen_10dp"
                            android:layout_weight="2"
                            android:background="@color/editArka"
                            android:ems="1"
                            android:lines="6"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="ENTRANCE"
                            android:layout_marginLeft="2dp"
                            android:layout_weight="2"
                            android:textColor="@color/editArka"
                            android:lines="1"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="2dp"
                            android:layout_weight="2"
                            android:background="@color/editArka"
                            android:ems="1"
                            android:lines="6"/>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <Button
                                android:id="@+id/btnchoose9"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@mipmap/b9"
                                android:padding="@dimen/dimen_45dp"
                                android:onClick="onButtonClicked"
                                android:text=""
                                android:textColor="@android:color/black" />

                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <LinearLayout
                            android:id="@+id/adana"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            >

                            <Button
                                android:id="@+id/btnchoose11"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@mipmap/b11"
                                android:padding="@dimen/dimen_45dp"
                                android:onClick="onButtonClicked"
                                android:text=""
                                android:textColor="@android:color/black" />

                        </LinearLayout>

                    </LinearLayout>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:id="@+id/layout14"
                            android:layout_width="wrap_content"
                            android:layout_marginTop="90dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            >

                            <Button
                                android:id="@+id/btnchoose14"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@mipmap/b14"
                                android:padding="@dimen/dimen_45dp"
                                android:onClick="onButtonClicked"
                                android:text=""
                                android:textColor="@android:color/black" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimen_10dp"
                                android:layout_marginLeft="@dimen/dimen_10dp"
                                android:background="@color/editArka"
                                android:ems="6"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:text="TOÄ°LETS"
                                android:lines="2"/>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimen_10dp"
                                android:layout_marginLeft="@dimen/dimen_10dp"
                                android:background="@color/editArka"
                                android:ems="6"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:text="BAR"
                                android:lines="2"/>
                            <Button
                                android:id="@+id/coklubtnsesim"
                                android:layout_marginLeft="40dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@color/btncolor"
                                android:onClick="onTamamClicked"
                                android:text="@string/cbmulti"
                                /> </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/layout12"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dimen_40dp"
                            android:orientation="horizontal"
                            >

                            <Button
                                android:id="@+id/btnchoose12"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@mipmap/b12"
                                android:padding="@dimen/dimen_45dp"
                                android:onClick="onButtonClicked"
                                android:text=""
                                android:textColor="@android:color/black" />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_toRightOf="@+id/layout12">
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/dimen_10dp"
                                    android:layout_weight="2"
                                    android:background="@color/editArka"
                                    android:ems="1"
                                    android:lines="6"/>
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <Button
                                        android:id="@+id/btnchoose5"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:background="@mipmap/b5"
                                        android:padding="@dimen/dimen_45dp"
                                        android:onClick="onButtonClicked"
                                        android:text=""
                                        android:textColor="@android:color/black" />

                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <Button
                                    android:id="@+id/btnchoose3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@mipmap/b3"
                                    android:padding="@dimen/dimen_45dp"
                                    android:onClick="onButtonClicked"
                                    android:text=""
                                    android:textColor="@android:color/black" />


                            </LinearLayout>
                        </LinearLayout>

                    </RelativeLayout>

                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ikincilayout"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:layout_toRightOf="@+id/entrancelayout"
                    android:layout_marginLeft="@dimen/dimen_20dp"
                    android:gravity="center"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >

                        <Button
                            android:id="@+id/btnchoose8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@mipmap/b8"
                            android:padding="@dimen/dimen_45dp"
                            android:onClick="onButtonClicked"
                            android:text=""
                            android:textColor="@android:color/black" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >

                        <Button
                            android:id="@+id/btnchoose6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/dimen_45dp"
                            android:background="@mipmap/b6"
                            android:onClick="onButtonClicked"
                            android:text=""
                            android:textColor="@android:color/black" />


                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >

                        <Button
                            android:id="@+id/btnchoose4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@mipmap/b4"
                            android:onClick="onButtonClicked"
                            android:text=""
                            android:padding="@dimen/dimen_45dp"
                            android:textColor="@android:color/black" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >

                        <Button
                            android:id="@+id/btnchoose2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@mipmap/b2"
                            android:padding="@dimen/dimen_45dp"
                            android:onClick="onButtonClicked"
                            android:text=""
                            android:textColor="@android:color/black" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >

                        <Button
                            android:id="@+id/btnchoose1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/dimen_45dp"
                            android:background="@mipmap/b1"
                            android:onClick="onButtonClicked"
                            android:text=""
                            android:textColor="@android:color/black" />


                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="25dp"
                        android:layout_marginLeft="@dimen/dimen_10dp"
                        android:background="@color/editArka"
                        android:ems="8"
                        android:gravity="center"
                        android:layout_marginRight="20dp"
                        android:textColor="@color/white"
                        android:text="BAR"
                        android:lines="2"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_margin="@dimen/dimen_10dp"
                    android:layout_toRightOf="@+id/ikincilayout"
                    android:layout_height="wrap_content">

                </LinearLayout>

            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>
